{
    "name": "dbt/odbc-driver",
    "description": "ODBC Driver for Laravel 5.5+",
    "license": "MIT",
    "authors": [
        {
            "name": "tck",
            "email": "ben@thecreativekind.co.uk"
        },
        {
            "name": "Daniel S. Deboer",
            "email": "daniel.s.deboer@gmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "laravel",
        "database",
        "odbc",
        "driver"
    ],
    "require": {
        "php": "8.1.*|8.2.*|8.3.*",
        "ext-pdo": "*",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "orchestra/testbench": "^7.0|^10.0",
        "phpunit/phpunit": "^9.5|^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Dbt\\Odbc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dbt\\Odbc\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dbt\\Odbc\\OdbcServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit --no-coverage",
        "coverage": "phpunit --coverage-html coverage",
        "pint": "pint",
        "pint-dry": "pint --test",
        "ci": "phpunit --configuration=phpunit.ci.xml"
    },
    "config": {
        "sort-packages": true
    }
}
